<div class="breadCrumbs">
	<strong><a class="lightHyperLink" href="/">~</a></strong><%_ breadCrumbs.forEach((crumb, i) => { -%><%_ if (i < breadCrumbs.length - 1) { -%><span class="bread-crumb-nav-slash"style="display: inline">/</span><strong><a href="/<%=crumb.join('/')-%>"><%-crumb[crumb.length-1]%></a></strong><%_} -%><%_ });-%>
</div>
